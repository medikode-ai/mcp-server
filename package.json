    "mcp",
    "model-context-protocol",
    "healthcare",
    "medical-coding",
    "medikode",
    "claude",
    "cursor",
    "ai-assistant",
    "medical-ai"
  ],
  "author": "Medikode",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "node-fetch": "^2.7.0",
    "uuid": "^12.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
