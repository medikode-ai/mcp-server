{
  "name": "@medikode/mcp-server",
  "version": "1.2.1",
  "description": "Model Context Protocol (MCP) server for Medikode healthcare SaaS platform",
  "main": "mcp-server-stdio.js",
  "bin": {
    "medikode-mcp-server": "./mcp-server-stdio.js"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medikode/mcp-server.git"
  },
  "homepage": "https://mcp.medikode.ai",
  "bugs": {
    "url": "https://github.com/medikode/mcp-server/issues"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --inspect=0.0.0.0:9229 index.js",
    "dev:debug": "nodemon --inspect-brk=0.0.0.0:9229 index.js",
    "debug": "node --inspect-brk index.js",
